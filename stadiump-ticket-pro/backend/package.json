    "@types/qrcode": "^1.5.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
